<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Curr Control Trials Cardiovasc Med</journal-id><journal-title>Current Controlled Trials in Cardiovascular Medicine</journal-title><issn pub-type="ppub">1468-6708</issn><issn pub-type="epub">1468-6694</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">15918901</article-id><article-id pub-id-type="pmc">1166569</article-id><article-id pub-id-type="publisher-id">1468-6708-6-10</article-id><article-id pub-id-type="doi">10.1186/1468-6708-6-10</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Exercise training improves aerobic endurance and musculoskeletal fitness in female cardiac transplant recipients</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Haykowsky</surname><given-names>Mark</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>mark.haykowsky@ualberta.ca</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Riess</surname><given-names>Kenneth</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>kriess@ualberta.ca</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Figgures</surname><given-names>Linda</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>lfiggures@cha.ab.ca</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Kim</surname><given-names>Daniel</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>dkim@cha.ab.ca</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Warburton</surname><given-names>Darren</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>darrenwb@interchange.ubc.ca</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Jones</surname><given-names>Lee</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>lee.w.jones@duke.edu</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Tymchak</surname><given-names>Wayne</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>waynetymchak@cha.ab.ca</email></contrib></contrib-group><aff id="I1"><label>1</label>Faculty of Rehabilitation Medicine, University of Alberta, Edmonton, Alberta, Canada</aff><aff id="I2"><label>2</label>Division of Cardiology, Faculty of Medicine, University of Alberta, Edmonton, Alberta, Canada</aff><aff id="I3"><label>3</label>School of Human Kinetics, University of British Columbia, Vancouver, British Columbia, Canada</aff><aff id="I4"><label>4</label>Program of Cancer Prevention Detection and Control, Duke University Medical Center, Durham, North Carolina, USA</aff><pub-date pub-type="ppub"><year>2005</year></pub-date><pub-date pub-type="epub"><day>26</day><month>5</month><year>2005</year></pub-date><volume>6</volume><issue>1</issue><fpage>10</fpage><lpage>10</lpage><ext-link ext-link-type="uri" xlink:href="http://cvm.controlled-trials.com/content/6/1/10"/><history><date date-type="received"><day>10</day><month>5</month><year>2005</year></date><date date-type="accepted"><day>26</day><month>5</month><year>2005</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2005 Haykowsky et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2005</copyright-year><copyright-holder>Haykowsky et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
